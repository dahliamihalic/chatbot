{
    "welcome": {
      "patterns": ["hi", "hello", "hey", "good morning", "good afternoon", "good evening"],
      "responses": [
        "Hello! This is a test chatbot version 0.0.0.1! Not all functions will be implemented within the final build, so please be patient!"
      ]
    },
    "bye": {
      "patterns": ["bye", "goodbye", "see you", "good night", "later"],
      "responses": [
        "Have a good one!",
        "Despacito",
        "Adios!",
        "Ciao!",
        "Buonanotte",
        "Auf Wiedersehen",
        "La revedere"
      ]
    },
    "thanks": {
      "patterns": ["thanks", "thank you", "appreciate it"],
      "responses": [
        "Awesome sauce!",
        "No problemo!",
        "cool",
        "Yep! Any other questions?",
        "Got anything else?"
      ]
    },
    "help": {
      "patterns": ["help", "what can you do", "capabilities"],
      "responses": [
        "What do you need help on?",
        "Do you have anything to work on?",
        "Sorry! I can't help you right now- just kidding! What do you need?"
      ]
    },
    "country": {
        "patterns": ["USA, China, Canada, Japan, Germany, Korea, Mexico, Brazil"],
        "responses": [
            "You just said a country! Nice!"
        ]
    },
    "no": {
        "patterns": ["what is", "how is", "who is", "where is", "when is"],
        "responses": [
            "no"
        ]
    },
    "default": {
      "responses": [
        "The bot is currently broken. Please try again later."
      ]
    }
  } 